{
  "apiVersion": "1",
  "name": "b-blocks/bar-chart",
  "title": "Bar Chart",
  "description": "Gutenberg editing with easy-to-use, responsive bar-chart  more.",
  "category": "media",
  "keywords": [
    "bar-chart",
    "Rating",
    "Gutenberg"
  ],
  "textdomain": "bar-chart",
  "attributes": {
    "align": {
      "type": "string",
      "default": ""
    },
    "cId": {
      "type": "string",
      "default": ""
    },
    "myChart": {
      "type": "object",
      "default": {
        "width": {
          "type": "number",
          "widthValue": 95
        },
        "type": {
          "type": "string",
          "chartType": "bar"
        },
        "labels": {
          "type": "string",
          "default": "January"
        },
        "data": 21,
        "dataLabel": {
          "type": "string",
          "label": "T-Shirt"
        },
        "background": {
          "type": "string",
          "default": "#ffffff"
        },
        "border": {
          "color": {
            "type": "string",
            "default": "#000000",
            "description": "Border color."
          },
          "width": {
            "type": "number",
            "default": 4,
            "description": "Border width in pixels."
          }
        }
      }
    },
    "ratingScale": {
      "type": "number",
      "default": 5
    },
    "jsonData": {
      "type": "array",
      "default": [
        {
          "label": "DataBase JSON 1",
          "value": 23
        },
        {
          "label": "DataBase JSON 2",
          "value": 43
        },
        {
          "label": "DataBase JSON 3",
          "value": 55
        }
      ]
    },
    "xmlData": {
      "type": "array",
      "default": [
        {
          "label": "DataBase XML 1",
          "value": 12
        },
        {
          "label": "DataBase XML 2",
          "value": 34
        },
        {
          "label": "DataBase XML 2",
          "value": 34
        }
      ]
    },
    "csvData": {
      "type": "array",
      "default": [
        {
          "label": "DataBase Sheets Data",
          "value": 13
        },
        {
          "label": "DataBase Sheets Data 2",
          "value": 43
        },
        {
          "label": "DataBase Sheets Data 3",
          "value": 14
        },
        {
          "label": "DataBase Sheets Data 4",
          "value": 33
        }
      ]
    },
    "legend": {
      "position": {
        "type": "string",
        "default": "top",
        "description": "Choose legend position from the list."
      },
      "color": {
        "type": "string",
        "default": "#333333",
        "description": "Set legend text/label color."
      },
      "pointStyle": {
        "type": "boolean",
        "default": true,
        "description": "Enable or disable legend point style."
      }
    },
    "textShadow": {
      "type": "array",
      "default": []
    }
  },
  "supports": {
    "align": [
      "wide",
      "full"
    ],
    "html": false
  },
  "example": {
    "attributes": {
      "preview": true,
      "columns": {
        "desktop": 1,
        "tablet": 1,
        "mobile": 1
      }
    }
  },
  "editorScript": "file:../dist/editor.js"
}